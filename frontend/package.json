{
  "name": "pseudopoll",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "openapi:spec": "aws apigateway get-export --parameters extensions='apigateway' --rest-api-id $(terraform -chdir=../terraform output -raw rest_api_id) --stage-name $(terraform -chdir=../terraform output -raw rest_api_stage_name) --export-type oas30 --accepts application/yaml ./types/openapi/oas30.yaml",
    "openapi:typegen": "openapi-typescript ./types/openapi/oas30.yaml -o ./types/openapi/generated.d.ts"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@nuxt/devtools": "^1.0.8",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "nuxt": "^3.10.3",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2",
    "vue-router": "^4.3.0"
  },
  "dependencies": {
    "@auth/core": "^0.17.0",
    "@hebilicious/authjs-nuxt": "^0.3.5",
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@iconify-json/lucide": "^1.1.174",
    "@nuxt/ui": "^2.14.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@tanstack/vue-query": "^5.28.4",
    "@tanstack/vue-query-devtools": "^5.28.4",
    "clsx": "^2.1.0",
    "date-fns": "^3.5.0",
    "mqtt": "^5.4.0",
    "openapi-fetch": "^0.8.2",
    "tailwind-merge": "^2.2.2",
    "valibot": "^0.27.1"
  }
}
