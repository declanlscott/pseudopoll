{
  "name": "pseudopoll",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "openapi:spec": "aws apigateway get-export --parameters extensions='apigateway' --rest-api-id $(terraform -chdir=../terraform output -raw rest_api_id) --stage-name $(terraform -chdir=../terraform output -raw rest_api_stage_name) --export-type oas30 --accepts application/yaml ./openapi/specs/oas30.yaml",
    "openapi:typegen": "openapi-typescript ./openapi/specs/oas30.yaml -o ./openapi/types/generated.d.ts"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@nuxt/devtools": "^1.0.6",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@tanstack/eslint-plugin-query": "^4.38.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "nuxt": "^3.9.1",
    "openapi-typescript": "^6.7.3",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "vue": "3.3.13",
    "vue-eslint-parser": "^9.3.2",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@auth/core": "^0.17.0",
    "@hebilicious/authjs-nuxt": "^0.3.5",
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxt/ui": "^2.11.1",
    "@tanstack/vue-query": "^5.17.4",
    "clsx": "^2.1.0",
    "date-fns": "^3.1.0",
    "openapi-fetch": "^0.8.2",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  }
}
